{
  "name": "youtoolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "canvas-confetti": "^1.9.3",
    "lucide-react": "^0.525.0",
    "moment": "^2.30.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.19",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run postbuild",
    "postbuild": "cp public/sitemap.xml build/ && cp public/.htaccess build/ 2>/dev/null || true && react-snap",
    "postinstall": "patch-package",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lighthouse": "lighthouse http://localhost:3000 --view --only-categories=performance",
    "serve": "npm run build && npx serve -s build",
    "perf": "npm run build && concurrently --kill-others \"npx serve -s build -l 3000\" \"sleep 3 && npm run lighthouse\"",
    "optimize-images": "chmod +x optimize-images.sh && ./optimize-images.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/moment": "^2.11.29",
    "@types/styled-components": "^5.1.34",
    "patch-package": "^8.0.1",
    "react-snap": "^1.23.0"
  },
  "reactSnap": {
    "source": "build",
    "minifyHtml": {
      "collapseWhitespace": false
    },
    "puppeteerExecutablePath": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "skipThirdPartyRequests": true,
    "inlineCss": false,
    "include": [
      "/",
      "/tools",
      "/tools/thumbnail-downloader",
      "/tools/video-analyzer",
      "/tools/channel-analyzer",
      "/tools/tag-generator",
      "/tools/keyword-analyzer",
      "/tools/thumbnail-tester",
      "/tools/thumbnail-analyzer",
      "/tools/channel-comparer",
      "/tools/comment-downloader",
      "/tools/comment-picker",
      "/tools/playlist-analyzer",
      "/tools/qr-code-generator",
      "/tools/subscribe-link-generator",
      "/tools/youtube-calculator",
      "/tools/color-palette",
      "/tools/color-picker-from-image",
      "/tools/outlier-finder",
      "/tools/channel-consultant",
      "/tools/channel-id-finder",
      "/tools/moderation-checker",
      "/tools/banner-downloader",
      "/tools/profile-picture-downloader",
      "/tools/youtool-playbooks",
      "/about",
      "/blog",
      "/privacy-policy",
      "/terms-of-service",
      "/glossary",
      "/guides"
    ]
  }
}
